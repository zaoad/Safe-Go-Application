<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="ButtonStyle">
        <item name="background">@drawable/button_background_blue</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>

    </style>
    <style name="ViewStyle">
        <item name="background">@drawable/view_background</item>
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/titillium_semibold</item>
        <item name="android:textSize">@dimen/viewfont</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">@dimen/marginview</item>
        <item name="elevation">1dp</item>
    </style>

</resources>